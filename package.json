{
  "name": "mini-dict-generator",
  "version": "0.0.0",
  "description": "compress key-value dict",
  "access": "public",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "elzup/mini-dict-generator",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "deploy": "npm run build && yarn publish",
    "lint": "eslint --ext .ts,.tsx",
    "test": "yarn test:jest",
    "test:jest": "jest src",
    "test:cov": "jest src --coverage",
    "test:badges": "yarn test:cov && istanbul-badges-readme",
    "prepare": "husky install",
    "depcruise": "depcruise --config .dependency-cruiser.js --include-only \"^src\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "release": "yarn build && yarn publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "utils",
    "personal"
  ],
  "devDependencies": {
    "@elzup/tsconfig": "^1.0.2",
    "@swc/core": "1.2.207",
    "@swc/jest": "0.2.21",
    "@types/jest": "28.1.4",
    "@types/node": "17.0.31",
    "babel-jest": "28.0.3",
    "eslint": "8.20.0",
    "eslint-config-nzap": "6.0.1",
    "husky": "7.0.4",
    "jest": "28.1.0",
    "lint-staged": "12.5.0",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "ts-node": "^10.9.1",
    "tsd": "0.21.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@elzup/kit": "^3.5.4"
  }
}
